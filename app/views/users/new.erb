<div class="form">



<%= form_for @user do |f|%>
<div class="form-group">
<%= f.label :first_name %>
<%= f.text_field :first_name, placeholder: "first name", :class => "form-control"  %><br>
</div>

<div class="form-group">
<%= f.label :last_name %>
<%= f.text_field :last_name, placeholder: "last name", :class => "form-control"%><br>
</div>

<div class="form-group">
<%= f.label :email %>
<%= f.email_field :email, placeholder: "name@example.com", :class => "form-control"  %><br>
</div>

<div class="form-group">
<%= f.label "Password" %>
<%= f.password_field :password, placeholder: "password", :class => "form-control", :id =>"exampleFormControlSelect1"%><br>
</div>

<div class="form-group">
<%= f.label "Password Confirmation" %>
<%= f.password_field :password_confirmation, placeholder: "confirm password", :class => "form-control", :id =>"exampleFormControlSelect1"%><br>
</div>

<div class="form-group">
<%= f.submit "Submit", :class => "btn btn-secondary" %>
<% end %>
</div>



  <div class="form-group">
    <label for="exampleFormControlSelect1">Example select</label>
    <select class="form-control" id="exampleFormControlSelect1">
      <option>1</option>
      <option>2</option>
      <option>3</option>
      <option>4</option>
      <option>5</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleFormControlSelect2">Example multiple select</label>
    <select multiple class="form-control" id="exampleFormControlSelect2">
      <option>1</option>
      <option>2</option>
      <option>3</option>
      <option>4</option>
      <option>5</option>
    </select>
  </div>
  <div class="form-group">
    <label for="exampleFormControlTextarea1">Example textarea</label>
    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
  </div>
</form>

</div>
